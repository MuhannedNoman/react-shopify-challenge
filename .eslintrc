{

    "plugins": ["react", "prettier"],
    "extends": [
        "react-app",
        "airbnb",
        "prettier",
    ],
    "rules": {
        "prettier/prettier": [
          "error", 
          {
            "singleQuote": true 
          }
        ],
        "react/jsx-filename-extension": [ 
          "warn", 
          {
            "extensions": [".js", ".jsx"] 
          }
        ]
      },
    "env": {
        "browser": true
    }
}